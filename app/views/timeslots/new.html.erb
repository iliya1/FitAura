<%= render layout:"shared/panel" do %>
  <h1>Add a time</h1>
  <%= form_for @timeslot, url: studio_class_timeslots_path(@studio, @studio_class) do |f| %>
    <%= f.text_field :start_date, class: "datepicker" %>
    <%= f.text_field :end_date, class: "datepicker" %>
    <%= f.text_field :start_time %>
    <%= f.text_field :duration %>
    <%= f.collection_check_boxes :days, Date::DAYNAMES, :to_s, :to_s  %>
    <%= f.submit %>
  <% end %>

<% end %>

<script>
 $(function() {
   $( ".datepicker" ).datepicker();
 });
</script>
